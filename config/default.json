{
  "plugins": {
    "./server/search-api": {},
    "./server/component-data": {},
    "./server/component-data/poll": {
      "options": {
        "enabled": false
      }
    },
    "./server/plugins/webapp": {
      "options": {
        "pageTitle": "Electrode Explorer",
        "paths": {
          "/{args*}": {
            "content": {
              "module": "./server/app"
            }
          }
        }
      }
    }
  },

  "githubApi": {
    "version": "3.0.0",
    "pathPrefix": "/api/v3",
    "protocol": "https",
    "host": "github.com"
  },

  "ui": {
    "basePath": "/explorer",
    "meta": {
      "name": "Electrode Explorer",
      "logo": "/explorer/img/electrode.png"
    },
    "github": {
      "protocol": "https",
      "basePath": "github.com"
    },
    "timeout": 10000
  },

  "ORGS": [

  ],

  "REPOS_USAGE_INCLUDE": [

  ],

  "REPOS_USAGE_EXCLUDE": [

  ],

  "MODULE_PREFIXES_INCLUDE": [

  ],

  "automaticUpdate": true,

  "NPM_WAITING_TIME": 300000,

  "POLL_INTERVAL": 1800000,

  "GHACCESS_TOKEN_NAME": "GHACCESS_TOKEN"
}
